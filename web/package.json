    "@sentry/tracing": "^7.17.4",
    "@sentry/vue": "^7.17.4",
    "aws-amplify": "4.3.43",
    "d3-hierarchy": "^3.1.2",
    "d3-selection": "^3.0.0",
    "jest-canvas-mock": "^2.5.0",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.21",
    "moment": "^2.15.2",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "node-sql-parser": "^4.6.4",
    "plotly.js-dist-min": "^2.24.3",
    "quasar": "^2.7.5",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-polyfill-node": "^0.10.2",
    "rudder-sdk-js": "^2.20.0",
    "vue": "^3.2.37",
    "vue-drag-resize": "^1.5.4",
