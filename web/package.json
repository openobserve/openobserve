    "@types/d3-selection": "^3.0.5",
    "@types/js-cookie": "^3.0.2",
    "@types/jsdom": "^16.2.14",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.45",
    "@types/plotly.js-dist-min": "^2.3.1",
    "@vitejs/plugin-vue": "^3.0.1",
