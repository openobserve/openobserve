{
  "extends": "./tsconfig.app.json",
  "exclude": [],
  "compilerOptions": {
    "allowJs": true,
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "composite": true,
    "types": ["node"],
    "baseUrl": ".",
    "downlevelIteration": true,
  }
}
