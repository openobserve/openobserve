          "queryType": "sql",
          "queries": [
            {
              "query": "SELECT key as \"x_axis_1\", AVG(retries) as \"y_axis_1\" FROM \"triggers\" WHERE module = 'alert' AND retries > 0 [WHERE_CLAUSE_ADDITIONAL] GROUP BY key ORDER BY y_axis_1 DESC LIMIT 20",
              "customQuery": true,
              "fields": {
                "stream": "triggers",
