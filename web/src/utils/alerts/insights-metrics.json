        {
          "id": "Panel_Alert_Volume",
          "type": "bar",
          "title": "Trigger Volume Over Time",
          "description": "Total triggers executed over time - Click and drag to zoom, right-click to filter",
          "config": {
            "show_legends": false,
            "unit": "custom",
            "decimals": 0,
            "line_thickness": 2,
            "show_symbol": false,
            "line_interpolation": "smooth",
