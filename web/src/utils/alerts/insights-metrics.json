    {
      "tabId": "correlation",
      "name": "Correlation & Grouping",
      "panels": [
        {
          "id": "Panel_Alert_Correlation",
          "type": "table",
          "title": "Alert Correlation Analysis",
          "description": "Alerts that fire together (within 60 seconds) - Grouping candidates",
          "config": {
            "wrap_table_cells": false,
            "table_dynamic_columns": true,
            "columns": [
              {
                "name": "alert_1",
                "label": "Alert 1"
              },
              {
                "name": "alert_2",
                "label": "Alert 2"
              },
              {
                "name": "correlation_count",
                "label": "Correlation Count"
              },
              {
                "name": "avg_time_diff_microseconds",
                "label": "Avg Time Diff (\u03bcs)"
              }
            ]
          },
          "queryType": "sql",
          "queries": [
            {
              "query": "WITH alert_times AS ( SELECT key, _timestamp, LAG(key) OVER (ORDER BY _timestamp) as prev_alert, _timestamp - LAG(_timestamp) OVER (ORDER BY _timestamp) as time_diff FROM \"triggers\" WHERE module = 'alert' [WHERE_CLAUSE_ADDITIONAL] ) SELECT prev_alert as alert_1, key as alert_2, COUNT(*) as correlation_count, AVG(time_diff) as avg_time_diff_microseconds FROM alert_times WHERE time_diff < 60000000 AND prev_alert IS NOT NULL AND prev_alert != key GROUP BY prev_alert, key HAVING correlation_count >= 5 ORDER BY correlation_count DESC LIMIT 30",
              "customQuery": true,
              "fields": {
                "stream": "triggers",
                "stream_type": "logs",
                "x": [
                  {
                    "label": "Alert 1",
                    "alias": "alert_1",
                    "column": "alert_1",
                    "color": null,
                    "isDerived": true
                  },
                  {
                    "label": "Alert 2",
                    "alias": "alert_2",
                    "column": "alert_2",
                    "color": null,
                    "isDerived": true
                  }
                ],
                "y": [
                  {
                    "label": "Correlation Count",
                    "alias": "correlation_count",
                    "column": "correlation_count",
                    "color": null,
                    "isDerived": true
                  },
                  {
                    "label": "Avg Time Diff (\u03bcs)",
                    "alias": "avg_time_diff_microseconds",
                    "column": "avg_time_diff_microseconds",
                    "color": null,
                    "isDerived": true
                  }
                ],
                "z": []
              }
            }
          ],
          "layout": {
            "x": 0,
            "y": 0,
            "w": 24,
            "h": 12,
            "i": 1
          }
        },
        {
          "id": "Panel_Alert_Storms",
          "type": "table",
          "title": "Alert Storms Detection",
          "description": "Time periods with abnormally high alert volume (>10 alerts in 5 min)",
          "config": {
            "wrap_table_cells": false,
            "table_dynamic_columns": true
          },
          "queryType": "sql",
          "queries": [
            {
              "query": "WITH minute_buckets AS ( SELECT DATE_TRUNC('minute', to_timestamp(_timestamp/1000000)) as storm_time, COUNT(*) as alert_count, COUNT(DISTINCT key) as unique_alerts, array_agg(DISTINCT key) as alerts_involved FROM \"triggers\" WHERE module = 'alert' [WHERE_CLAUSE_ADDITIONAL] GROUP BY storm_time ) SELECT storm_time, alert_count, unique_alerts, alerts_involved FROM minute_buckets WHERE alert_count > 10 ORDER BY alert_count DESC LIMIT 50",
              "customQuery": true,
              "fields": {
                "stream": "triggers",
                "stream_type": "logs",
                "x": [
                  {
                    "label": "Storm Time",
                    "alias": "storm_time",
                    "column": "storm_time",
                    "color": null,
                    "isDerived": true
                  }
                ],
                "y": [
                  {
                    "label": "Alert Count",
                    "alias": "alert_count",
                    "column": "alert_count",
                    "color": null,
                    "isDerived": true
                  },
                  {
                    "label": "Unique Alerts",
                    "alias": "unique_alerts",
                    "column": "unique_alerts",
                    "color": null,
                    "isDerived": true
                  },
                  {
                    "label": "Alerts Involved",
                    "alias": "alerts_involved",
                    "column": "alerts_involved",
                    "color": null,
                    "isDerived": true
                  }
                ],
                "z": []
              }
            }
          ],
          "layout": {
            "x": 0,
            "y": 12,
            "w": 24,
            "h": 10,
            "i": 2
          }
        },
