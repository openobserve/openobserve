              "query": "WITH trigger_metrics AS ( SELECT _timestamp, CASE WHEN status='completed' THEN 1 ELSE 0 END as firing_count FROM \"triggers\" WHERE module = 'alert' [WHERE_CLAUSE_ADDITIONAL] ) SELECT histogram(_timestamp) as time_bucket, (SUM(firing_count) * 100.0 / COUNT(*)) as firing_rate FROM trigger_metrics GROUP BY time_bucket ORDER BY time_bucket ASC",
              "customQuery": true,
              "fields": {
                "stream": "triggers",
